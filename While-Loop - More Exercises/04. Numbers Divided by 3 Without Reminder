using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace _04.Numbers_Divided_by_3_With
{
    internal class Program
    {
        static void Main(string[] args)
        {
            for (int i = 1; i <= 100; i++)
            {
                if (i % 3 == 0)
                {
                    Console.WriteLine(i);
                }
            }
        }
    }
}
